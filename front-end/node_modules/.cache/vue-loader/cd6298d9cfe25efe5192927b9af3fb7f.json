ate: {{ job.start_date }}</p>\r\n              </div>\r\n              \r\n                <button class=\"auto button is-rounded is-link is-centered\" @click= \"saveJob(job)\">Save</button>\r\n              \r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: \"JobListing\",\r\n\r\n  data() {\r\n    return {\r\n      jobs: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.getJobs();\r\n  },\r\n  methods: {\r\n    saveJob(job) {\r\n      this.$root.$data.saved_jobs.push(job);\r\n    },\r\n    async getJobs() {\r\n      try {\r\n        let response = await axios.get(\"/api/job\");\r\n        this.job = response.data;\r\n        return true;\r\n      } catch (error) {\r\n        console.log(error);\r\n      }\r\n    }\r\n  },\r\n}\r\n</script>\r\n<style>\r\n  .card-content {\r\n    text-align: left;\r\n    padding: 10px 30px;\r\n    height: 150px;\r\n  }\r\n.jobs {\r\n \r\n  margin-top: 20px;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: space-evenly;\r\n}\r\n.job {\r\n  margin: 10px;\r\n  margin-top: 50px;\r\n  width: 30%\r\n}\r\n.button {\r\n  text-align: center;\r\n  margin-bottom: 10px;\r\n}\r\n</style>"]}]}